{
  "extends": [
    "standard",
    "eslint:recommended",
    "plugin:react/recommended"
    ],
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-tag-spacing": ["error", {
      "closingSlash": "never",
      "beforeSelfClosing": "always",
      "afterOpening": "never",
      "beforeClosing": "never"
    }],
    "semi": ["error", "never"],
    "no-console": "warn",
    "indent": "warn",
    "comma-dangle": "warn",
    "object-curly-spacing": ["warn", "always"]
  }
}
