{
  "name": "react_seed_project",
  "version": "1.0.0",
  "description": "React seed project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --mode=development && webpack-dev-server --mode=development --hot --color --watch --open",
    "build": "webpack --mode=production --progress",
    "build:stats": "webpack --config=webpack.stats.config.js --env --mode=production --progress --profile",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-standard",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "keywords": [
    "react",
    "seed"
  ],
  "author": "Nick Lontorfos",
  "license": "MIT",
  "dependencies": {
    "dotenv-webpack": "^1.5.7",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.1",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.3",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^8.0.4",
    "node-sass": "^4.10.0",
    "prettier-standard": "^8.0.1",
    "sass": "^1.14.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-monitor": "^1.0.14"
  }
}
